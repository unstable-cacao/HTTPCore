{
	"name": "unstable-cacao/http-core",
    "description": "Wrapper for https requests to another server",
	"type": "library",
	"license": "MIT",
	"homepage": "hhttps://github.com/unstable-cacao/HTTPCore",
	
	"authors": [
		
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=7.1",
		"unstable-cacao/structura": "^1.0",
		"unstable-cacao/traitor": "^1.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^6.2",
		"oktopost/unit-cover": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "HTTPCore\\": "Source/"
        }
    },
    "scripts": {
        "test": "./vendor/phpunit/phpunit/phpunit --dont-report-useless-tests",
        "test-cover": "./vendor/phpunit/phpunit/phpunit --dont-report-useless-tests --coverage-html ./build/cover  && ./vendor/oktopost/unit-cover/cover /build/logs/clover.xml 95"
    }
}
